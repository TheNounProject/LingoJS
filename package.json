    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.1",
    "prettier": "^2.2.1"
